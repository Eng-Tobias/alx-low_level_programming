#!/bin/bash
# This script runs a C file through the preprocessor and saves the output in a file named 'c'

# Check if the CFILE variable is set
if [ -z "$CFILE" ]; then
    echo "Error: CFILE variable is not set."
    exit 1
fi

# Run the preprocessor and save the output to 'c'
gcc -E "$CFILE" -o c

